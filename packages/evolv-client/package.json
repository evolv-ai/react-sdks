{
  "name": "@evolv-delivery-tools/client",
  "version": "0.1.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs,iife --dts",
    "dev": "tsup src/index.ts --format esm,cjs,iife --dts --watch",
    "postinstall": "node ./postinstall.js"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evolv-ai/delivery.git"
  },
  "devDependencies": {
    "config": "workspace:*",
    "tsconfig": "workspace:*",
    "tsup": "^5.12.5",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "@evolv/javascript-sdk": "^1.19.4",
    "ulid": "^2.3.0"
  },
  "bugs": {
    "url": "https://github.com/evolv-ai/delivery/issues"
  },
  "homepage": "https://github.com/evolv-ai/delivery#readme",
  "description": "Javascript client for connecting to Evolv AI",
  "author": "timothy.cubbedge@evolv.ai"
}
